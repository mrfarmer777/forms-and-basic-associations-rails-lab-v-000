<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_id %>
  <%= f.select(:genre_id, Genre.all.collect {|g| [g.name, g.id]}) %>
  <%= f.label :notes%>
  <%= f.text_field

  <%= f.submit %>
<% end %>
